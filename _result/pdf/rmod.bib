	Title = {Towards a Tool-based Development Methodology for Pervasive Computing Applications},
	Url = {http://hal.inria.fr/docs/00/68/32/10/PDF/TSE-2010-06-0178.R3\_Cassou.pdf},
	Volume = {38},

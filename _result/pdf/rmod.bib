	Publisher = {ACM},
	Series = {PPPJ '14},
	Title = {Fork/Join Parallelism in the Wild: Documenting Patterns and Anti-Patterns in Java Programs using the Fork/Join Framework},
	Url = {http://rmod.inria.fr/archives/papers/DeWa14a-PPPJ14-ForkJoin.pdf},
